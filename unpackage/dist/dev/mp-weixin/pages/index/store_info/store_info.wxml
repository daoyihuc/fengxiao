<view class="content"><view class="new_bg"><image src="{{data.StoreInfo.logo}}" mode></image><view class="new_card"><view class="img_li" style="margin-top:0rpx;"><image src="../../../static/img/index/dianpu.png" mode></image><text style="padding-left:15rpx;">{{data.StoreInfo.store_name}}</text></view><view class="img_li"><image src="../../../static/img/index/dizhi.png" mode></image><text style="padding-left:15rpx;">{{''+data.StoreInfo.province+data.StoreInfo.city+data.StoreInfo.district+data.StoreInfo.address+''}}</text></view><view class="three_text"><view class="left">充值余额：<text style="color:#FA5F3E;">{{data.ResidualIntegral}}</text></view><view class="left">总消费：<text style="color:#FA5F3E;">{{data.ConsumptionPoints}}</text></view><view class="right">{{'共享'+data.ProfitNumber+'分参与'}}</view></view><view class="li_money"><view class="small_left">距离返现相差金额：</view><view class="small_left"><text style="color:#FA5F3E;">{{data.ThreeTotalOtherMoney}}</text></view></view><view class="li_money"><view class="small_left">失效金额(三个月内)：</view><view class="small_left"><text style="color:#FA5F3E;">{{data.InvalidAmount}}</text></view></view><view class="li_money"><view class="small_left">失效时间（三个月内）：</view><view class="small_left"><text style="color:#FA5F3E;">{{data.invalid_endtime}}</text></view></view></view></view><view class="card"><view class="card_top"><view class="row"></view><text>近期消费</text></view></view><block wx:if="{{isShow==true}}"><loading vue-id="54ab2c9d-1" bind:__l="__l"></loading></block><view class="withdrawalrecord"><block wx:for="{{logList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="record_list"><view class="left"><view class="title">{{item.title}}</view><view class="time">{{"订单号："+item.order_sn}}</view><view class="time">{{item.time}}</view></view><view class="right"><view>{{item.money}}</view><view class="time">{{item.statust==0?'待审核':item.statust==1?'审核成功':item.statust==2?'审核失败':''}}</view></view></view></block></view><block wx:if="{{logList.length==0}}"><nodata vue-id="54ab2c9d-2" colors="{{colors}}" title="暂无明细" bind:__l="__l"></nodata></block></view>