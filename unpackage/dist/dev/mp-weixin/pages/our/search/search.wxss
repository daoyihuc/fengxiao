.content .search {
  background-color: #F4F4F4;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  margin: 0 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.content .search image {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-left: 20rpx;
  padding: 10rpx;
}
.content .search_list .text {
  font-size: 12px;
  padding: 20rpx;
  border-bottom: 1px solid #eee;
}
.content .search_list .search_li {
  display: -webkit-box;
  display: -webkit-flex;
